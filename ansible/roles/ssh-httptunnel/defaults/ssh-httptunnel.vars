#-*- mode: yaml -*-

# BEGIN Variables for ssh-httptunnel
# hostname or IP of the target SSH server
TARGET_HOST: 127.0.0.1
# SSH port for the target SSH server
TARGET_PORT: 22

# listen port for httptunnel server
HTTPTUNNEL_PORT: 9000

# URL path prefix for NGINX location configuration
sshtunnel_prefix: sshtunnel
# Location of basic auth credentials
htpasswd_location: /etc/nginx/.htpasswd
# END Variables for ssh-httptunnel
